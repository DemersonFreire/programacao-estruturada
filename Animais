#include <iostream>

using namespace std;

string a, b, c;

void input();
void conclusao();

int main() {
    input();
    conclusao();

    return(0);
}

void input() {
    cin >> a >> b >> c;
}

void conclusao() {
    if (a == "vertebrado") {
        if (b == "ave") {
            if (c == "carnivoro")
                cout << "aguia" << endl;
            else
                cout << "pomba" << endl;
        }
        else {
            if (c == "onivoro")
                cout << "homem" << endl;
            else
                cout << "vaca" << endl;
        }
    }
    else {
        if (b == "inseto") {
            if (c == "hematofago")
                cout << "pulga" << endl;
            else
                cout << "lagarta" << endl;
        }
        else {
            if (c == "hematofago")
                cout << "sanguessuga" << endl;
            else
                cout << "minhoca" << endl;
        }
    }

}
